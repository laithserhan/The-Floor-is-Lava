pico-8 cartridge // http://www.pico-8.com
version 5
__lua__
levels_qty = 12

function _init()
 -- screens:
 -- 0: intro screen
 -- 1: main game
 -- 2: end of game
 cartdata("sp500_pipe_dream")
 high_score=dget(0) or 100
 score = 100

 screen = 0

	levels={}
 for l=1,levels_qty do
  levels[l]={}
  levels[l].gap_start = flr(rnd(120))
  levels[l].gap_width = flr(rnd(5))+10
  if l % 2 == 0 then
   levels[l].dir = 1
  else
   levels[l].dir = -1
  end
 end
 door_x = flr(rnd(128-8))
 p = {}
 p.level = flr(rnd(levels_qty/2))+1
 p.x = flr(rnd(60))+30
end

function _update()
 if screen == 0 then
  if btnp(4) then
   screen = 1
  end
 elseif screen == 2 then
  if btnp(4) then
   screen = 0
  end
 else --screen is 1
  for l in all(levels) do
   l.gap_start += l.dir
  
   if l.gap_start < 0 then
    l.gap_start = 128
   elseif l.gap_start > 128 then
    l.gap_start = 0
   end
  end

  if p.level < 1 then
   if score > high_score then
    dset(0, score)
   end
   screen = 2
  elseif p.level > 12 then
   screen = 2
  else
   score -= 0.1
  end

 	if btn(0) then
	  if btnp(2) then
	   jump(-5)
	  end
		 p.x -= 1
	 elseif btn(1) then
	  if btnp(2) then
	   jump(5)
	  end
	 	p.x += 1
	 end

	 if p.x < 0 then
 	 p.x = 128
	 elseif p.x > 128 then
	  p.x = 0
	 end

  if screen == 1 then
  	p_l = levels[p.level]
	  if p.x > p_l.gap_start and
	     (p.x+8) < p_l.gap_start+p_l.gap_width then
	   p.level += 1
    sfx(1)
   end
  end
 end
end

function _draw()
 cls()
 
 if screen == 0 then
  print("welcome to", 50, 30)
  color(15)
  print("pipe dream", 50, 40)
  color(7)
  print("by sue and philip", 40, 50)
  print("high score: "..high_score, 44, 60)
  print("press a to start", 41, 80)
 elseif screen == 1 then
  -- score
  print("score: "..score, 85, 0)
  -- door
  spr(2, door_x, 0)
  -- levels
  for n,l in pairs(levels) do
   l_y = 116/levels_qty*n
   gap_end = l.gap_start+l.gap_width
   -- before gap
 		line(max(0, gap_end-128),
	 	     l_y,
		 				 l.gap_start,
			 			 l_y)
   -- after gap
   line(gap_end,
   		   l_y,
  	 	   128,
  		    l_y)  			 
  end
 
  -- fire
  for fire=0,128,9 do
   spr(1, fire, 120)
  end
 
  -- person
  if p.x > 120 then
   spr(0,
       p.x-128,
       116/levels_qty*p.level-7)
  end
  spr(0,
      p.x,
      116/levels_qty*p.level-7)
 else
   print("game over", 50, 30)
   print("your score: "..score, 45, 40)
   print("high score: "..high_score, 45, 50)
 end
end

function jump(x_delta)
 if p.level < 1 then --door
  if p.x+2 > door_x and
     p.x+6 < door_x+8 then
   screen = 2
  end
 else
 	p_l = levels[p.level - 1]
  gap_end = p_l.gap_start + p_l.gap_width
  if p.x+2 > p_l.gap_start and
	    p.x+6 < gap_end then
	   sfx(2)
	   p.level -= 1
    p.x += x_delta
	 end
 end
end
